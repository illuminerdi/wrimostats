<h1>Listing word_wars</h1>

<table>
  <tr>
    <th>User</th>
    <th>Title</th>
    <th>Description</th>
    <th>Can has snaps</th>
  </tr>

<% @word_wars.each do |word_war| %>
  <tr>
    <td><%=h word_war.user_id %></td>
    <td><%=h word_war.title %></td>
    <td><%=h word_war.description %></td>
    <td><%=h word_war.can_has_snaps %></td>
    <td><%= link_to 'Show', word_war %></td>
    <td><%= link_to 'Edit', edit_word_war_path(word_war) %></td>
    <td><%= link_to 'Destroy', word_war, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New word_war', new_word_war_path %>